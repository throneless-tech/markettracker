{
  "name": "markettracker",
  "description": "Payload project created from ts-blank template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "build:server": "tsc",
    "build": "npm run generate:types && npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "test": "true",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(js|jsx|css|less|scss|ts|tsx|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/theme-tools": "^2.1.0",
    "@documenso/nodemailer-resend": "^4.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.7",
    "@fontsource/outfit": "^5.0.7",
    "@fontsource/zilla-slab": "^5.0.7",
    "@payloadcms/bundler-webpack": "^1.0.4",
    "@payloadcms/db-mongodb": "^1.0.4",
    "@payloadcms/plugin-cloud": "^2.2.1",
    "@payloadcms/richtext-slate": "^1.0.7",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-query": "^5.12.2",
    "@tanstack/react-table": "^8.10.7",
    "@tanstack/react-virtual": "^3.2.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.15.0",
    "humanparser": "^2.7.0",
    "node-xlsx": "^0.23.0",
    "papaparse": "^5.4.1",
    "payload": "^2.2.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.18.0",
    "react-intersection-observer": "^9.5.3",
    "react-pdf": "^7.5.0",
    "react-toastify": "^9.1.3",
    "resend": "^4.0.1",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.53.0",
    "@types/express": "^4.17.17",
    "@types/humanparser": "^1.1.6",
    "@types/nodemailer": "^6.4.16",
    "@types/react-datepicker": "^4.19.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^13.23.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
