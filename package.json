{
  "name": "markettracker",
  "description": "Payload project created from ts-blank template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm run generate:types && npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/theme-tools": "^2.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.7",
    "@fontsource/outfit": "^5.0.7",
    "@fontsource/zilla-slab": "^5.0.7",
    "@payloadcms/plugin-cloud": "^2.2.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.15.0",
    "humanparser": "^2.7.0",
    "node-xlsx": "^0.23.0",
    "payload": "^1.11.7",
    "react": "^18.2.0",
    "react-datepicker": "^4.18.0",
    "react-pdf": "^7.5.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/humanparser": "^1.1.6",
    "@types/react-datepicker": "^4.19.0",
    "@types/react-router-dom": "^5.3.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
